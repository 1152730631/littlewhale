<block wx:if="{{$taroCompReady}}">
    <view class="{{anonymousState__temp2}}" style="{{anonymousState__temp3}}">
        <template is="renderClosureComponent" data="{{...anonymousState__temp}}"></template>
        <scroll-view scroll-y="{{true}}">
            <view class="scrollx" style="{{anonymousState__temp4}}" bindtouchstart="anonymousFunc0" bindtouchmove="anonymousFunc1" bindtouchend="anonymousFunc2">
                <slot></slot>
            </view>
        </scroll-view>
    </view>
</block>
<template name="renderClosureComponent">
    <block>
        <block wx:if="{{type === 'default'}}">
            <template is="renderDefaultComponent" data="{{..._$_$anonymousState__temp}}"></template>
        </block>
        <block wx:elif="{{type === 'verb'}}">
            <template is="renderVerbComponent" data="{{..._$_$anonymousState__temp2}}"></template>
        </block>
        <block wx:elif="{{type === 'center'}}">
            <template is="renderCenterComponent" data="{{..._$_$anonymousState__temp3}}"></template>
        </block>
        <view wx:else></view>
    </block>
</template>