<template name="renderDefaultComponent">
    <block>
        <scroll-view scroll-x="{{true}}" class="{{bgColorClassName + ' nav'}}" scroll-with-animation="{{true}}" scroll-left="{{scrollLeft}}">
            <view wx:key="index" class="{{'cu-item ' + (activeTab === index ? activeColor + ' cur' : '')}}" bindtap="onClickTab" wx:for="{{tabs}}" wx:for-item="item" wx:for-index="index" data-e-tap-so="this" data-e-tap-a-a="{{index}}">
                <block wx:if="{{item.icon}}"><text class="{{'cuIcon-' + item.icon + ' margin-right-xs'}}"></text>
                </block><text>{{item.text}}</text>
            </view>
        </scroll-view>
    </block>
</template>
