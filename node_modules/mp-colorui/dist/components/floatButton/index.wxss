.float_button__mask {
  position: fixed;
  width: 100vw;
  height: 100vh;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background-color: rgba(0, 0, 0, 0.4);
  z-index: 99; }
.float_button__fixed {
  position: fixed;
  right: 50rpx;
  bottom: 200rpx; }
.float_button__content {
  position: relative; }