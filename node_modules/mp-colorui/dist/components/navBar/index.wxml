<block wx:if="{{$taroCompReady}}">
    <view class="{{anonymousState__temp}}" style="{{anonymousState__temp2}}">
        <view class="action">
            <block wx:if="{{leftIcon}}">
                <block wx:key="index" wx:for="{{leftIcon}}" wx:for-item="item" wx:for-index="index"><text class="{{(item.icon ? 'cuIcon-' + item.icon : '') + ' ' + (item.color ? TEXT_COLOR_LIST[item.color] : '')}}" bindtap="onClickLeftIcon" data-e-tap-so="this" data-e-tap-a-a="{{index}}"></text><text class="{{'' + (item.color ? TEXT_COLOR_LIST[item.color] : '')}}" bindtap="onClickLeftIcon" data-e-tap-so="this" data-e-tap-a-a="{{index}}">{{item.text}}</text>
                </block>
            </block>
        </view>
        <view class="content text-bold">{{title}}</view>
        <view class="action">
            <block wx:if="{{rightIcon}}">
                <block wx:key="index" wx:for="{{rightIcon}}" wx:for-item="item" wx:for-index="index"><text class="{{(item.icon ? 'cuIcon-' + item.icon : '') + ' ' + (item.color ? TEXT_COLOR_LIST[item.color] : '')}}" bindtap="onClickRightIcon" data-e-tap-so="this" data-e-tap-a-a="{{index}}"></text>
                </block>
            </block>
        </view>
    </view>
</block>