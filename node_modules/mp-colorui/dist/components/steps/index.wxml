<block wx:if="{{$taroCompReady}}">
    <view class="{{anonymousState__temp}}" style="{{anonymousState__temp2}}">
        <view class="{{'cu-item ' + (step > index ? activeColorClassName : '')}}" wx:key="index" wx:for="{{steps}}" wx:for-item="item" wx:for-index="index">{{stepTitlePosition === "top" ? item.title : ""}}<text class="{{(stepIconType === 'number' ? 'num' : '') + ' ' + (stepIconType === 'custom' ? 'cuIcon-' + item.icon : '')}}" data-index="{{index + 1}}"></text>{{stepTitlePosition === "bottom" ? item.title : ""}}</view>
    </view>
</block>