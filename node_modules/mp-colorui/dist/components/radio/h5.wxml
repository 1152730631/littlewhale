<block wx:if="{{$taroCompReady}}">
    <view>
        <block>
            <block wx:if="{{type === 'list'}}">
                <template is="renderClosureListComponent" data="{{...anonymousState__temp}}"></template>
            </block>
            <block wx:else>
                <view class="h5-radio-group">
                    <block>
                        <block wx:if="{{type === 'form'}}">
                            <view class="cu-form-group">
                                <view class="title">{{title}}</view>
                                <view class="{{directionClassName}}">
                                    <view class="padding-xs" wx:key="value" wx:for="{{loopArray75}}" wx:for-item="item" wx:for-index="index"><text class="padding-right-sm">{{item.$original.key}}</text>
                                        <view bindtap="anonymousFunc0" class="{{item.$loopState__temp3}}" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}">
                                            <view class="h5-radio-wrapper">
                                                <view class="h5-radio-input h5-radio-input-checked"></view>
                                            </view>
                                        </view>
                                    </view>
                                </view>
                            </view>
                        </block>
                        <block wx:else>
                            <view class="{{directionClassName}}">
                                <view class="padding-xs" wx:key="value" wx:for="{{loopArray75}}" wx:for-item="item" wx:for-index="index"><text class="padding-right-sm">{{item.$original.key}}</text>
                                    <view bindtap="anonymousFunc0" class="{{item.$loopState__temp3}}" data-e-tap-so="this" data-e-tap-a-a="{{item._$indexKey}}">
                                        <view class="h5-radio-wrapper">
                                            <view class="h5-radio-input h5-radio-input-checked"></view>
                                        </view>
                                    </view>
                                </view>
                            </view>
                        </block>
                    </block>
                </view>
            </block>
        </block>
    </view>
</block>
<template name="renderClosureListComponent">
    <block>
        <list-radio compid="{{$compid__864}}"></list-radio>
    </block>
</template>